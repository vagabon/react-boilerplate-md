{
  "name": "@vagabond-inc/react-boilerplate-md",
  "version": "1.3.1",
  "description": "Boilerplate Material Design on React with MUI",
  "private": false,
  "scripts": {
    "lint": "eslint \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "tsc": "tsc",
    "test": "jest",
    "test:json": "npm run test -- --json --outputFile=src/jest-test-results.json --coverage",
    "coverage": "npm run test -- --coverage ",
    "format": "npm run lint && npm run tsc",
    "ci": "npm run format && npm run coverage -- --testResultsProcessor jest-sonar-reporter",
    "sonar": "npm run ci && npx sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:dev": "npm run build -- --watch",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm i --force && npm run format && npm run test:json && npm run build"
  },
  "dependencies": {
    "@emotion/react": "~11.14.0",
    "@mui/icons-material": "~6.4.3",
    "@mui/material": "~6.4.3",
    "@mui/x-date-pickers": "~7.25.0",
    "@mui/x-tree-view": "~7.25.0",
    "dayjs": "~1.11.13",
    "i18next": "~24.2.2",
    "i18next-browser-languagedetector": "~8.0.2",
    "mui-markdown": "~1.2.5",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-i18next": "~15.4.0",
    "react-router-dom": "~7.1.5",
    "uuid": "~11.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.26.7",
    "@babel/preset-react": "~7.26.3",
    "@babel/preset-typescript": "~7.26.0",
    "@rollup/plugin-babel": "~6.0.4",
    "@rollup/plugin-commonjs": "~28.0.2",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-multi-entry": "~6.0.1",
    "@rollup/plugin-node-resolve": "~16.0.0",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~12.1.2",
    "@storybook/addon-essentials": "~8.5.3",
    "@storybook/addon-interactions": "~8.5.3",
    "@storybook/addon-jest": "~8.5.3",
    "@storybook/addon-links": "~8.5.3",
    "@storybook/blocks": "~8.5.3",
    "@storybook/react": "~8.5.3",
    "@storybook/react-vite": "~8.5.3",
    "@testing-library/react": "~16.2.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "~8.23.0",
    "@typescript-eslint/parser": "~8.23.0",
    "babel-jest": "~29.7.0",
    "babel-plugin-direct-import": "~1.0.0",
    "babel-plugin-react-compiler": "~19.0.0-beta-714736e-20250131",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "~7.37.4",
    "eslint-plugin-react-compiler": "~19.0.0-beta-714736e-20250131",
    "eslint-plugin-react-hooks": "~5.1.0",
    "fs-extra": "~11.3.0",
    "glob": "~11.0.1",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "path-browserify": "~1.0.1",
    "prism-react-renderer": "~2.4.1",
    "rollup": "~4.34.5",
    "rollup-plugin-copy": "~3.5.0",
    "rollup-plugin-dts": "~6.1.1",
    "rollup-plugin-peer-deps-external": "~2.2.4",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-scss": "~4.0.1",
    "sass": "~1.84.0",
    "sonarqube-scanner": "~4.2.6",
    "storybook": "~8.5.3",
    "typescript": "~5.7.3",
    "vite-tsconfig-paths": "~5.1.4"
  },
  "peerDependencies": {
    "@emotion/react": "~11.14.0",
    "@mui/icons-material": "~6.4.3",
    "@mui/material": "~6.4.3",
    "@mui/x-date-pickers": "~7.25.0",
    "@mui/x-tree-view": "~7.25.0",
    "dayjs": "~1.11.13",
    "i18next": "~24.2.2",
    "i18next-browser-languagedetector": "~8.0.0",
    "mui-markdown": "~1.2.5",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-i18next": "~15.4.0",
    "react-router-dom": "~6.24.0",
    "uuid": "~11.0.5"
  },
  "overrides": {
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
