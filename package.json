{
  "name": "@vagabond-inc/react-boilerplate-md",
  "version": "1.1.3",
  "description": "Boilerplate Material Design on React with MUI",
  "private": false,
  "scripts": {
    "lint": "eslint \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "tsc": "tsc",
    "test": "jest ",
    "test:json": "npm run test -- --json --outputFile=src/jest-test-results.json --coverage",
    "coverage": "npm run test -- --coverage ",
    "format": "npm run lint && npm run tsc",
    "ci": "npm run format && npm run coverage -- --testResultsProcessor jest-sonar-reporter",
    "sonar": "npm run ci && npx sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:dev": "npm run build -- --watch",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm i && npm run format && npm run test:json && npm run build"
  },
  "dependencies": {
    "@emotion/react": "~11.11.4",
    "@fontsource/roboto": "~5.0.13",
    "@mui/icons-material": "~5.15.18",
    "@mui/material": "~5.15.18",
    "@mui/x-date-pickers": "~7.5.0",
    "@mui/x-tree-view": "~7.5.0",
    "dayjs": "~1.11.11",
    "i18next": "~23.11.4",
    "i18next-browser-languagedetector": "~8.0.0",
    "mui-markdown": "~1.1.13",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-i18next": "~14.1.1",
    "react-router-dom": "~6.23.1",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.24.5",
    "@babel/preset-react": "~7.24.1",
    "@babel/preset-typescript": "~7.24.1",
    "@rollup/plugin-babel": "~6.0.4",
    "@rollup/plugin-commonjs": "~25.0.7",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~11.1.6",
    "@storybook/addon-essentials": "~8.1.1",
    "@storybook/addon-interactions": "~8.1.1",
    "@storybook/addon-jest": "~8.1.1",
    "@storybook/addon-links": "~8.1.1",
    "@storybook/blocks": "~8.1.1",
    "@storybook/react": "~8.1.1",
    "@storybook/react-vite": "~8.1.1",
    "@testing-library/react": "~15.0.7",
    "@typescript-eslint/eslint-plugin": "~7.9.0",
    "@typescript-eslint/parser": "~7.9.0",
    "babel-jest": "~29.7.0",
    "babel-plugin-direct-import": "^1.0.0",
    "eslint": "~8.57.0",
    "eslint-plugin-react": "~7.34.1",
    "eslint-plugin-react-hooks": "~4.6.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.15",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "path-browserify": "^1.0.1",
    "rollup": "~4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "~6.1.1",
    "rollup-plugin-peer-deps-external": "~2.2.4",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-scss": "~4.0.0",
    "sass": "~1.77.2",
    "sonarqube-scanner": "~3.5.0",
    "storybook": "~8.1.1",
    "typescript": "~5.4.5",
    "vite-tsconfig-paths": "~4.3.2"
  },
  "peerDependencies": {
    "@emotion/react": "~11.11.4",
    "@mui/icons-material": "~5.15.17",
    "@mui/material": "~5.15.17",
    "@mui/x-date-pickers": "~7.5.0",
    "@mui/x-tree-view": "~7.5.0",
    "dayjs": "~1.11.11",
    "i18next": "~23.11.3",
    "i18next-browser-languagedetector": "~7.2.1",
    "mui-markdown": "~1.1.13",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-i18next": "~14.1.0",
    "react-router-dom": "~6.23.0",
    "uuid": "~9.0.0"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
