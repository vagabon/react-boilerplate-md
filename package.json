{
  "name": "@vagabond-inc/react-boilerplate-md",
  "version": "0.0.43",
  "description": "Boilerplate Material Design on React with MUI",
  "private": false,
  "type": "module",
  "scripts": {
    "lint": "eslint \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "tsc": "tsc",
    "test": "jest  ",
    "test:json": "npm run test -- --json --outputFile=src/jest-test-results.json --coverage",
    "coverage": "npm run test -- --coverage ",
    "format": "npm run lint && npm run tsc",
    "ci": "npm run format && npm run coverage -- --testResultsProcessor jest-sonar-reporter",
    "sonar": "npm run ci && npx sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && rollup -c --bundleConfigAsCjs",
    "build:dev": "npm run build -- --watch",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm run format && npm run test:json && npm i && npm run build"
  },
  "dependencies": {
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@fontsource/roboto": "~5.0.8",
    "@mui/icons-material": "~5.15.10",
    "@mui/material": "~5.15.10",
    "@mui/x-date-pickers": "~6.19.4",
    "@mui/x-tree-view": "~6.17.0",
    "dayjs": "~1.11.10",
    "i18next": "~23.9.0",
    "i18next-browser-languagedetector": "~7.2.0",
    "mui-markdown": "~1.1.13",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-i18next": "~14.0.5",
    "react-router-dom": "~6.22.1",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.23.9",
    "@babel/preset-react": "~7.23.3",
    "@babel/preset-typescript": "~7.23.3",
    "@rollup/plugin-commonjs": "~25.0.7",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~11.1.6",
    "@storybook/addon-essentials": "~7.6.16",
    "@storybook/addon-interactions": "~7.6.16",
    "@storybook/addon-jest": "~7.6.16",
    "@storybook/addon-links": "~7.6.16",
    "@storybook/blocks": "~7.6.16",
    "@storybook/react": "~7.6.16",
    "@storybook/react-vite": "~7.6.16",
    "@testing-library/react": "~14.2.1",
    "@typescript-eslint/eslint-plugin": "~7.0.1",
    "@typescript-eslint/parser": "~7.0.1",
    "babel-jest": "~29.7.0",
    "eslint": "~8.56.0",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "path-browserify": "^1.0.1",
    "rollup": "~4.12.0",
    "rollup-plugin-dts": "~6.1.0",
    "rollup-plugin-peer-deps-external": "~2.2.4",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-scss": "~4.0.0",
    "sass": "~1.71.0",
    "sonarqube-scanner": "~3.3.0",
    "storybook": "~7.6.16",
    "typescript": "~5.3.3",
    "vite-tsconfig-paths": "~4.3.1"
  },
  "peerDependencies": {
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@fontsource/roboto": "~5.0.8",
    "@mui/icons-material": "~5.15.10",
    "@mui/material": "~5.15.10",
    "@mui/x-date-pickers": "~6.19.4",
    "@mui/x-tree-view": "~6.17.0",
    "dayjs": "~1.11.10",
    "i18next": "~23.9.0",
    "i18next-browser-languagedetector": "~7.2.0",
    "mui-markdown": "~1.1.13",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-i18next": "~14.0.5",
    "react-router-dom": "~6.22.1",
    "sass": "~1.71.0",
    "uuid": "~9.0.0"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "18"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react/prop-types": "off",
      "@typescript-eslint/triple-slash-reference": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/anchor-has-content": "off",
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/no-restricted-imports": [
        "warn",
        {
          "name": "react-redux",
          "importNames": [
            "useSelector",
            "useDispatch"
          ],
          "message": "Use typed hooks `useAppDispatch` and `useAppSelector` instead."
        }
      ]
    },
    "ignorePatterns": [
      "src/index.ts",
      "dist/**",
      "**/*babel*",
      "**/*.stories.*",
      "**/*Store.*",
      "**/*.test.*",
      "**/setupTests.*",
      "node_modules/**/*.ts?(x)"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
